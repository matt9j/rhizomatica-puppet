!
! osmo-mgw (media gateway) MGCP configuration
!
mgcp
  bind ip <%= @mgw_controlplane_ip_address %>
  bind port 2427
  rtp port-range 4002 16000
  rtp bind-ip <%= @mgw_dataplane_ip_address %>
  rtp ip-probing
  rtp ip-tos 184
  sdp audio payload number 98
  sdp audio payload name GSM
  number endpoints 31
  loop 0
  force-realloc 1
  rtcp-omit
  rtp-patch ssrc
  rtp-patch timestamp
