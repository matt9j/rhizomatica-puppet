!
! OsmoMSC configuration saved from vty
!
log stderr
 logging filter all 1
 logging color 1
 logging print category 1
 logging print category-hex 0
 logging print level 1
 logging print file basename last
 logging level all everything
 logging level pag debug
 logging level msc notice
 logging level smpp everything
 logging level lsms everything
 logging level vlr debug
 logging level sgs debug
 logging level linp notice
 logging level lss7 notice
 logging level bssap notice	
line vty
 no login
!
network
 network country code <%= @mcc %>
 mobile network code <%= @mnc %>
 short name <%= @network_name %>
 long name <%= @network_name %>
 encryption a5 0
 rrlp mode none
 mm info 1
 ! authentication required
msc
 mgw remote-ip <%= @mgw_controlplane_ip_address %>
 mgw remote-port 2427
 mgw local-port 2728
 no assign-tmsi
 auth-tuple-max-reuse-count 3
 auth-tuple-reuse-on-error 1
 mncc external /tmp/msc_mncc
 check-imei-rqd early
mncc-int
 default-codec tch-f fr
 default-codec tch-h amr
smpp
 ! The ip parameter is documented but will not parse.
 ! local-tcp-ip 127.0.0.1:2775
 local-tcp-port 2775
 policy closed
 smpp-first
 esme OSMPP
  no alert-notifications
  password <%= @smsc_password %>
  default-route
 esme ISMPP
  no alert-notifications
  password <%= @smsc_password %>
 esme NOTIFY
  password <%= @smpp_password %>
  no deliver-src-imsi
sgs
 local-ip <%= @msc_ip_address %>
 local-port 29118
